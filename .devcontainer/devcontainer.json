{
    "name": "Laradock",
    "dockerComposeFile": "../laradock/docker-compose.yml",
    "remoteUser": "laradock",
    "extensions": [
      "onecentlin.laravel-extension-pack"
    ],
    "runServices": [
      "nginx",
      "postgres",
      "pgadmin"
    ],
    "initializeCommand": "ls && git submodule update && ls",
    "service": "workspace",
    "workspaceFolder": "/var/www",
    "shutdownAction": "stopCompose",
    "postCreateCommand": "uname -a"
  }
